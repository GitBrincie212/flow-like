{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "StorageItem",
	"type": "object",
	"required": ["last_modified", "location", "size"],
	"properties": {
		"location": {
			"type": "string"
		},
		"last_modified": {
			"type": "string"
		},
		"size": {
			"type": "integer",
			"format": "uint",
			"minimum": 0.0
		},
		"e_tag": {
			"type": ["string", "null"]
		},
		"version": {
			"type": ["string", "null"]
		}
	}
}
