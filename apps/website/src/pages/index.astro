---
import { edges, nodes } from "../assets/board.json";
import { EmptyBackendProvider } from "../components/backend-provider";
import Layout from "../layouts/Layout.astro";
import "@tm9657/flow-like-ui/global.css";
import "@xyflow/react/dist/style.css";
---

<head>
	<Fragment slot="head">
		<link rel="preload" as="fetch" href={"/board.json"} crossorigin="anonymous" />
	</Fragment>
</head>
<Layout>
	<EmptyBackendProvider data="/board.json" client:load />
</Layout>
